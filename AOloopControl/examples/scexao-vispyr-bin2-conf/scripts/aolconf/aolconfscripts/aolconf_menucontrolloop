 	 loopmaxlim=${value}
 	 echo "$loopmaxlim" > ./conf/param_loopmaxlim.txt
  	 tmux send-keys -t aol${LOOPNUMBER}-ctr "aolsetmaxlim ${loopmaxlim}" C-m
	 aoconflogext "set max limit ${loopmaxlim}"
