	;;


	atgupc)
	exec 3>&1;
	AUTOTUNEGAINupdateCoeff=$(dialog --inputbox "AUTOTUNEGAINupdateCoeff" 0 0 "$AUTOTUNEGAINupdateCoeff" 2>&1 1>&3);
	exec 3>&-;
	echo "$AUTOTUNEGAINupdateCoeff" > ./conf/param_AUTOTUNEGAINupdateCoeff.txt
