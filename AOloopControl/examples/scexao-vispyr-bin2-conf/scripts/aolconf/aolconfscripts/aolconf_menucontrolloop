readshmim aol${LOOPNUMBER}_wfsmask
readshmim aol${LOOPNUMBER}_wfsref0
im=aol${LOOPNUMBER}_wfsmask/itot(aol${LOOPNUMBER}_wfsmask)
imd=im-aol${LOOPNUMBER}_wfsref0
readshmim aol${LOOPNUMBER}_wfszpo_00
cpsh imd aol${LOOPNUMBER}_wfszpo_00
exitCLI
EOF
;;


	wr0)
# set WFS ref to measured
